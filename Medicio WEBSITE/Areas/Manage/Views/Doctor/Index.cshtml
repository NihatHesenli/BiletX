

@model List<Doctors>
@{
    int count = 0;
}



<div class="container">


<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Position</th>
      <th scope="col">Image</th>
    </tr>
  </thead>
  <tbody>

      @foreach(var item in Model)
            {
       count++;
      <tr>
      <th scope="row">@count</th>
      <td>@item.FullName</td>
      <td>@item.Position</td>
      <td> <img src="~/Upload/Doctor/@item.ImgUrl" /></td>
    </tr> 
            }

  </tbody>
</table>

















</div>
